<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:gui="clr-namespace:MMKiwi.ProjDash.GUI"
             x:Class="MMKiwi.ProjDash.GUI.App"
             x:DataType="gui:App"
             RequestedThemeVariant="Default">
    <TrayIcon.Icons>
        <TrayIcons>
            <TrayIcon Icon="/Assets/Icon.ico" 
                      ToolTipText="Project Dashboard" Command="{Binding ToggleCommand}">
                <TrayIcon.Menu>
                    <NativeMenu>
                        <NativeMenuItem Header="Projects" Menu="{Binding Menu^}" />
                        <NativeMenuItemSeparator />
                        <NativeMenuItem Header="Show Window" Command="{Binding ShowCommand}" IsVisible="{Binding ShowCommand.CanExecute^}" />
                        <NativeMenuItem Header="Hide Window" Command="{Binding HideCommand}" IsVisible="{Binding HideCommand.CanExecute^}"  />
                        <NativeMenuItem Header="Exit" Command="{Binding ExitCommand}" />
                    </NativeMenu>
                </TrayIcon.Menu>
            </TrayIcon>
        </TrayIcons>
    </TrayIcon.Icons>
    <Application.DataTemplates>
        <gui:ViewLocator />
    </Application.DataTemplates>
    <Application.Styles>
        <FluentTheme />
    </Application.Styles>
