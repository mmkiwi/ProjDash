<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:iconEditors="clr-namespace:MMKiwi.ProjDash.ViewModel.IconEditors;assembly=MMKiwi.ProjDash.ViewModel"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:DataType="iconEditors:FileIconViewModel"
             Name="ParentControl"
             x:Class="MMKiwi.ProjDash.GUI.UserControls.FileIconEditor">
    <Button Content="Browse"
            Command="{Binding #ParentControl.BrowseForIconAsync}"
            VerticalAlignment="Center" />
</UserControl>
